############################################################
### This is the ASTRA's general configuration input file ###
############################################################


# Name for the directory in which the quantum chemistry's (QC) outputs will be stored. Those from DALTON, LUCIA and the hybrid integrals library.
qc_dir=QC

# Name for the directory in which ASTRA's outputs will be stored.
storage_dir=store



# Input file name for the DALTON orbitals calculation.
dalton_cfg_file=DALTON.INP

# Input file name for the molecular geometry needed for DALTON.
molecule_cfg_file=MOLECULE.INP



# Input file name for the LUCIA state-averaged calculation. Only needs to be set if the state-average calculation
# is of interest, and then both files 'lucia_cfg_file_state_ave' and 'lucia_cfg_file' will be used.
lucia_cfg_file_state_ave=LUCIA_SA.INP

# Input file name for the LUCIA CASCI calculation. Either with the state-averaged calculation or without it
# this file will always be used.
lucia_cfg_file=LUCIA.INP



# Name of the hybryd integration library, at the moment either GBTOlib or HybridIntegrals.
int_library=###(int_library)

# Name of the integrals file generated by GBTOlib. It must be present if int_library=GBTOlib.
scatci_int_file=moints

# Input file name for the GBTOlib integrals calculation. It must be present if int_library=GBTOlib.
scatci_cfg_file=SCATCI.INP

# Name of the integrals file generated by HybridIntegrals. It must be present if int_library=HybridIntegrals.
hybrid_int_file=form_integrals.out

# Inputs directory for HybridIntegrals. It must be present if int_library=HybridIntegrals.
hybrid_int_input_dir=prism_inputs



# Name of the molden file generated by DALTON, whch is replaced by the one generated by LUCIA if a SA-MCSCF calculation is performed.
molden_file=molden.inp

# Input file name for the external numerical basis.
ext_basis_file=EXTERNAL_BASIS_BSPLINES.INP

# Input file name for the close-coupling basis.
cc_cfg_file=CLSCPLNG.INP

# Input file name for the time-dependent Schrodinger equation (TDSE) propagation.
tdse_cfg_file=TDSE.INP###(tdse_label)
